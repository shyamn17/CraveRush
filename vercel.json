{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "public" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/instamart.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  